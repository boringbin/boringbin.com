{
  "$schema": "https://unpkg.com/wrangler@latest/config-schema.json",
  "name": "boringbin-dot-com",
  "compatibility_date": "2025-05-19",
  "assets": {
    "directory": "./public",
  },
  "observability": {
    "enabled": true,
  },
  "routes": [
    {
      "pattern": "boringbin.com",
      "custom_domain": true,
    },
